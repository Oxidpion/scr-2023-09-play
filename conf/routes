# phonerecords

# Products
#  /products - позволяет добавить продукт отправив соответствующий JSON
#  /products - позволяет обновить имеющийся продукт передав соответствующий JSON
#  /products/{id} - позволяет удалить продукт по id\
#  /products - возвращает список продуктов
#  /products?title="some title" - возвращает список продуктов отфильтрованных по полю title

POST   /products             @controllers.ProductController.create()
PUT    /products/:productId  @controllers.ProductController.update(productId)
DELETE /products/:productId  @controllers.ProductController.delete(productId)
GET    /products             @controllers.ProductController.search(filter: Option[models.filter.ProductFilter])
